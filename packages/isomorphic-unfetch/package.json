{
  "name": "isomorphic-unfetch",
  "version": "4.0.0",
  "description": "Switches between unfetch & node-fetch for client & server.",
  "files": [
    "index.js",
    "index.d.ts",
    "browser.js"
  ],
  "exports": {
    ".": {
      "import": "./index.mjs",
      "default": "./index.js"
    },
    "./browser": {
      "import": "./browser.mjs",
      "default": "./browser.js"
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "repository": "developit/unfetch",
  "browser": "browser.js",
  "main": "index.js",
  "types": "index.d.ts",
  "dependencies": {
    "node-fetch": "^3.2.0",
    "unfetch": "^4.2.0"
  }
}
